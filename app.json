{
  "expo": {
    "name": "TechEvent 2025",
    "slug": "techevent-2025",
    "version": "1.0.0",
    "description": "Official app for TechEvent 2025 Innovation Summit",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "primaryColor": "#8ec63f",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.techevent.app2025"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#06121d"
      },
      "package": "com.techevent.app2025"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
